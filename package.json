{
    "name": "mirage-actual",
    "exportedEnvVars": {
        "mirage_installed": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "true"
        },
        "FINDLIB": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/lib"
        },
        "version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "2.9.1"
        },
        "mirage_version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "2.9.1"
        },
        "PATH": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/bin"
        },
        "mirage_enable": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "enable"
        }
    },
    "version": "2.9.1",
    "dependencies": {
        "@opam-alpha/mirage-types-lwt": "*",
        "@opam-alpha/astring": "*",
        "@opam-alpha/mirage-types": ">= 2.8.0",
        "@opam-alpha/ipaddr": ">= 2.6.0",
        "@opam-alpha/logs": "*",
        "dependency-env": "https://github.com/npm-ml/dependency-env.git",
        "substs": "https://github.com/yunxing/substs.git",
        "@opam-alpha/functoria": ">= 1.1.0",
        "nopam": "https://github.com/yunxing/nopam.git",
        "opam-installer-bin": "https://github.com/yunxing/opam-installer-bin.git"
    },
    "scripts": {
        "postinstall": "eval $(dependencyEnv) && nopam && ocaml setup.ml -configure --prefix $opam_prefix --bindir $opam_bin && make && make install && (opam-installer --prefix=$opam_prefix || true)"
    },
    "peerDependencies": {
        "@opam-alpha/ocaml": ">= 4.02.3"
    }
}
